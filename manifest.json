{
  "manifest_version": 2,
  "name": "Always In Container",
  "description": "__MSG_extensionDescription__",
  "version": "1.0.4",
  "default_locale": "en",
  "icons": {
    "48": "icon.svg"
  },
  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "cookies",
    "management",
    "tabs",
    "webRequestBlocking",
    "webRequest"
  ],
  "web_accessible_resources": [
    "/togo/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "not_allowed",
  "applications": {
    "gecko": {
      "id": "{a1e9543e-5f73-4763-b376-04e53fd12cbd}",
      "strict_min_version": "57.0"
    }
  }
}
